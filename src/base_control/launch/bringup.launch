<launch>
	<!-- base_control launch -->
	<node pkg="base_control" type="serial_script.py" name="serial_controller"/>

	<!-- lidar launch -->
	<!-- <node pkg="hls_lfcd_lds_driver" type="hlds_laser_publisher" name="hlds_laser_publisher" output="screen">
		<param name="port" value="/dev/ttyUSB1"/>
		<param name="frame_id" value="laser"/>
	</node> -->
	<node name="rplidarNode"          pkg="rplidar_ros"  type="rplidarNode" output="screen">
		<param name="serial_port"         type="string" value="/dev/ttyUSB1"/>
		<param name="serial_baudrate"     type="int"    value="115200"/><!--A1/A2 -->
		<param name="frame_id"            type="string" value="laser"/> 
		<param name="inverted"            type="bool"   value="false"/>
		<param name="angle_compensate"    type="bool"   value="true"/>
		<param name="scan_mode"           type="string" value="Express"/>
	</node>
	<node pkg="iris_nav" type="transform.py" name="tf_broadcaster"/>
	<node pkg="iris_nav" type="rescan_script.py" name="rescan_node"/>
	
	<!-- visualization launch 
	<node name="gui" pkg="rviz" type="rviz" args="-d $(find iris_nav)/navi.rviz"/> -->

	<!-- emotion launch 
	<include file="$(find bs_arm_control)/launch/bs_arm_launch.launch"/> -->

	<!-- navigation launch -->
	<include file="$(find iris_nav)/launch/navigation.launch"/>
	
	


</launch>
